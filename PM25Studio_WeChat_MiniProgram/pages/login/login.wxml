<!--pages/login/login.wxml-->


<image class="top_img" src="https://dcdn.it120.cc/2019/12/29/1800092d-0ca9-4fcb-a5f0-46aa6be9b61f.jpg" mode="aspectFill">
  <view class="category-item personal-info">
    <view class="user-avatar" style="margin-top:-{{top_height}}px">
      <open-data class="avatar" type="userAvatarUrl"></open-data>
    </view>
    <view>
      <view class="user-name">
        <open-data type="userNickName"></open-data>
      </view>
    </view>
  </view>
</image>





<view class="container">


  <view class="care-container">
    <view class="order">
      <block wx:for="{{cache}}">
      
        <view class="detail-item" catchtap="{{item.tap}}">
          <image src="{{item.iconurl}}"></image>
          <text>{{item.title}}</text>
        </view>

      </block>
    </view>
  </view>


  <view class="category-item1">
    <block wx:for="{{device}}">
      <view class="detail-item" catchtap="{{item.tap}}">
        <image src="{{item.iconurl}}"></image>
        <text>{{item.title}}</text>
        <view class="detail-item-btn"></view>
      </view>
    </block>
  </view>



</view>