} */


 


.box_left{   
  display: inline-block;  
  width: 25%;  
  background: #f5f5f5;  
  margin: auto;
}  

 .box_left_items{  
  height: 40px;  
  line-height: 40px;  
  padding: 6px 0;  
  border-bottom: 1px solid #ddd;  
  text-align: center;  
  font-size: 14px; 
}  

/*左侧栏list的item被选中时*/  
 .box_left_items.active{  
  background: #fff;  
  color: #999999; 
  border-right: 3px solid #999;
}  

/*右侧栏主盒子*/  
.box_right{  
  position: absolute;  
  top: 0;  
  right: 0;  
  width: 75%;  
  height: auto;
  padding:10rpx 0rpx 20rpx 20rpx;  
  box-sizing: border-box;  
  background: #ebebeb ;  
}  

/*右侧栏list的item*/  
.box_right .box_right_items{  
  float: left;  
  width: 100%;  
  height: 450rpx;
  text-align: center;   
}  
.box_right .box_right_items image{  
  width: 200px;  
  height: 110px;  
  margin-top: 25px;  
}  
.box_right .box_right_items text{   
  display: block;  
  margin: 5rpx 0rpx 10rpx 0rpx;  
  font-size: 14px;  
  color: #505050;
 
} 

.nodata_text{
  color: #505050;
  font-size: 14px;  
  text-align: center;  
} 
